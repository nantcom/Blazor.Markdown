<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Tananut Songsiriwith</Authors>
    <Company>Nancom Limited Company</Company>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>Razor Component Library for Convert Markdown text to HTML DOM</Description>
    <PackageProjectUrl>https://github.com/nantcom/Blazor.Markdown</PackageProjectUrl>
    <PackageTags>Markdown,Blazor</PackageTags>
  </PropertyGroup>

  
  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="HtmlSanitizer" Version="5.0.376" />
    <PackageReference Include="Markdig" Version="0.23.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
